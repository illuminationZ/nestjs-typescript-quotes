{
    "version": "2",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/main.ts",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"

            ]
        }
    ],
}