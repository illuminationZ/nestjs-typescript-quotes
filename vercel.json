{
    "version": 2,
    "builds": [
        {
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"

            ]
        }
    ]
}